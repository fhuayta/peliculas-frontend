{
  "name": "agetic-vue-base-frontend",
  "version": "1.0.0",
  "private": true,
  "author": "dbarra@agetic.gob.bo",
  "scripts": {
    "build": "vue-cli-service build",
    "test": "vue-cli-service test:unit",
    "format": "npx prettier --write src/**/**/*",
    "lint": "vue-cli-service lint ./src",
    "report": "vue-cli-service build --report",
    "secure": "vue-cli-service --https --open serve",
    "start": "NODE_OPTIONS=--openssl-legacy-provider vue-cli-service --open serve --port 8080",
    "coverage": "jest --coverage",
    "check-node-version": "./node_modules/.bin/check-node-version",
    "prebuild": "npm run check-node-version",
    "prestart": "npm run check-node-version",
    "pretest": "npm run check-node-version",
    "postinstall": "npm run check-node-version",
    "prepare": "husky install"
  },
  "jest": {
    "collectCoverage": true,
    "testResultsProcessor": "jest-sonar-reporter"
  },
  "dependencies": {
    "@elartedelcodigo/check-node-version": "1.1.4",
    "axios": "1.4.0",
    "casbin": "5.6.0",
    "core-js": "3.8.3",
    "dayjs": "1.10.3",
    "system-appearance": "1.0.2",
    "v-mask": "2.3.0",
    "validate.js": "0.13.1",
    "vue": "2.7.14",
    "vue-router": "3.5.1",
    "vue-session": "1.0.0",
    "vuetify": "2.6.0",
    "vuex": "3.5.1",
    "webpack": "4.46.0",
    "zxcvbn": "4.4.2"
  },
  "devDependencies": {
    "@commitlint/cli": "12.1.4",
    "@commitlint/config-conventional": "12.1.4",
    "@dbetka/vue-material-icons": "0.2.10",
    "@vue/cli-plugin-babel": "4.5.8",
    "@vue/cli-plugin-eslint": "5.0.6",
    "@vue/cli-plugin-router": "4.5.8",
    "@vue/cli-plugin-unit-jest": "4.5.11",
    "@vue/cli-plugin-vuex": "4.5.8",
    "@vue/cli-service": "4.5.19",
    "@vue/eslint-config-airbnb": "5.3.0",
    "@vue/test-utils": "1.0.3",
    "babel-eslint": "10.1.0",
    "cache-loader": "4.1.0",
    "eslint": "7.23.0",
    "eslint-config-prettier": "8.3.0",
    "eslint-plugin-import": "2.26.0",
    "eslint-plugin-vue": "7.1.0",
    "husky": "6.0.0",
    "image-minimizer-webpack-plugin": "1.0.0",
    "imagemin": "8.0.1",
    "imagemin-optipng": "8.0.0",
    "jest": "26.6.3",
    "jest-sonar-reporter": "2.0.0",
    "node-sass": "6.0.1",
    "prettier": "1.19.1",
    "sass": "~1.32.0",
    "sass-loader": "10.3.0",
    "vue-template-compiler": "2.6.12",
    "vuetify-loader": "1.6.0",
    "webpack-bundle-analyzer": "4.7.0"
  },
  "engines": {
    "node": "^18",
    "npm": "^9"
  }
}
